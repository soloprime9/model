{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/gen",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "env": {
    "MODEL_NAME": "t5-base"
  }
}
